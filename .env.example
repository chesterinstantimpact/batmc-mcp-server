# BATMC MCP Server Configuration
# Copy this file to .env and fill in values

# Environment: "dev" or "prod"
BATMC_ENV=dev

# API URL (Render deployment or local)
BATMC_API_URL=https://batmc-lending-api.onrender.com
# BATMC_API_URL=http://localhost:8000  # For local development

# Supabase project credentials
BATMC_SUPABASE_URL=https://YOUR_PROJECT.supabase.co
BATMC_SUPABASE_ANON_KEY=your_anon_key_here

# User credentials (which partner is using this server)
BATMC_USER_EMAIL=chester@example.com
BATMC_USER_PASSWORD=your_password_here
BATMC_USER_NAME=Chester
